{
  "hosting": {
    "public": "gad_web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source": "/**",
        "headers": [
          {
            "key": "Content-Security-Policy",
            "value": "script-src 'self' 'unsafe-eval'; object-src 'self'"
          }
        ]
      }
    ]
  }
}
